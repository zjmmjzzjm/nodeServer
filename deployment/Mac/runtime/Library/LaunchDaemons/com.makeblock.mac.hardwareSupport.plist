<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist 
  PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" 
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Disabled</key><false/>
  <key>Label</key><string>com.makeblock.mac.hardwareSupport</string>
  <key>ProgramArguments</key>
  <array>
	  <string>/Applications/MakeBlock/IdeService/start</string>
  </array>
    <key>RunAtLoad</key>
        <true/>
    <key>KeepAlive</key>
        <true/>
    <key>WorkingDirectory</key>
        <string>/Applications/MakeBlock/IdeService/</string>
    <key>StandardErrorPath</key>
    <string>/tmp/makeblock.ideService.error.log</string>
    <key>StandardOutPath</key>
    <string>/tmp/makeblock.IdeService.access.log</string>
</dict>
</plist>
